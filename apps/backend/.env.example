# Backend environment variables (example)
# Copy to .env and fill with your real values.

PORT=3001
N8N_BASE_URL=http://localhost:5678/webhook

# Supabase project settings
SUPABASE_URL=https://YOUR_PROJECT_REF.supabase.co

# Keep this secret; do NOT commit real service keys
SUPABASE_SERVICE_KEY=YOUR_SUPABASE_SERVICE_ROLE_KEY

# Safe to expose to the client, but still avoid committing real values in repos
SUPABASE_ANON_KEY=YOUR_SUPABASE_ANON_KEY

# Backend JWT signing secret (recommended)
JWT_SECRET=REPLACE_WITH_A_LONG_RANDOM_SECRET
