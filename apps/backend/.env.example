# Backend environment variables (example)
# Copy this file to .env and replace the placeholders with your real values.
# NOTE: Do NOT commit a file containing real secrets to source control. Use
# `.env.example` to share variable names and placeholders only.

# Local dev server port
PORT=3001

# n8n webhook base (if you run a local n8n instance)
N8N_BASE_URL=http://localhost:5678/webhook

# Supabase project settings (replace YOUR_PROJECT_REF)
SUPABASE_URL=https://YOUR_PROJECT_REF.supabase.co

# Keep this secret; do NOT commit real service keys
# Use the Supabase "service_role" key on the server only
SUPABASE_SERVICE_KEY=REPLACE_WITH_SUPABASE_SERVICE_ROLE_KEY

# Public (anon) key - do not commit real values to public repos
SUPABASE_ANON_KEY=REPLACE_WITH_SUPABASE_ANON_KEY

# Backend JWT signing secret (recommended - at least 32 chars)
JWT_SECRET=REPLACE_WITH_A_LONG_RANDOM_SECRET
